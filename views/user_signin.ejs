<link rel="stylesheet" href="/css/signin.css">
<div class="container">
    <h1>SignIn </h1>
    <form action="/users/create-session" method="POST">
        <input type="email" name="email" required placeholder="email">
        <input type="password" name="password" placeholder="password" required>
        <input type="submit" value="sign in">
    </form>
    <a href="/users/forget-password">Forget Password</a>
    <hr style="height: 4px; background-color: rgb(2, 63, 56); border: none;">
    <h2>SignIn / SignUp using Google</h2>
    <p>
        <a href="/users/auth/google"> <button>Google SignIn / SignUp</button></a>
    </p>
</div>